{
  "name": "medialab-web",
  "version": "0.1.0",
  "description": "Media Laboratory webapp",
  "author": "NewsTeller team <https://newsteller.io>",
  "homepage": "https://news-teller.github.io/media-laboratory-web/",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.2",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "fg-loadcss": "^3.1.0",
    "react": "^17.0.2",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-router-transition": "^2.1.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "watch:css": "npx tailwindcss -i ./src/tailwind.css -o ./src/style.css --watch",
    "build:css": "NODE_ENV=production npx tailwindcss -i ./src/tailwind.css -o ./src/style.css",
    "start": "craco start ",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "build-popovers": "webpack -c ./popoversjs/webpack.config.js",
    "predeploy": "npm run build && npm run build-popovers",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "gh-pages": "^3.2.3",
    "postcss": "^7.0.39",
    "react-app-rewired": "^2.1.8",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "webpack-cli": "^4.8.0"
  }
}
